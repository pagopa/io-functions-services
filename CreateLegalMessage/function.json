{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "v1/legal-messages/{legalmail}",
      "methods": [
        "post"
      ]
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "queueName": "%MESSAGE_CREATED_QUEUE_NAME%",
      "connection": "INTERNAL_STORAGE_CONNECTION_STRING",
      "name": "createdMessage",
      "type": "queue",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/CreateLegalMessage/index.js"
}